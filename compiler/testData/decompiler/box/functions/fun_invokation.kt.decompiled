fun fn_true(): Boolean {
    return true
}

fun fn_answer(): Int {
    when {
        fn_true() -> {
            return 42
        }
        else -> {
            return 666
        }
    }
}

fun box(): String {
    val answer: Int = fn_answer()
    when {
        answer == 42 -> {
            return "OK"
        }
        else -> {
            return "FAIL"
        }
    }
}

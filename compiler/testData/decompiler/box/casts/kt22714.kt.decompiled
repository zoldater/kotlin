fun <T> Array<T>.getLength() : Int  {
    return this.size
}

fun Any.getLength() : Int  {
    return if ((this is Array<*>)) {
    this.size
}
else {
    -1
}
}

fun box() : String  {
    val array1 : Array<String> = arrayOf<String>("1", "2", "3")
    val array2 : Any = arrayOf<String>("1", "2", "3")
    if (array1.getLength<String>() + array2.getLength() != 6) {
        return "FAILURE"
    }
    return "OK"
}

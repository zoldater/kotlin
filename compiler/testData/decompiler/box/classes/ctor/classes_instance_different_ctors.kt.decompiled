class TestClassWithPrimaryAndSecondaryCtors {
    constructor() {
    }
    var intProp: Int = 42
    var stringProp: String = "42"
    constructor(intProp: Int) {
        this.intProp = intProp
        this.stringProp = intProp.toString()
    }
    constructor(stringProp: String) {
        this.stringProp = stringProp
    }
}
fun box(): String  {
    val testClassPrimary: TestClassWithPrimaryAndSecondaryCtors = TestClassWithPrimaryAndSecondaryCtors()
    val testClassSecondaryInt: TestClassWithPrimaryAndSecondaryCtors = TestClassWithPrimaryAndSecondaryCtors(777)
    val testClassSecondaryString: TestClassWithPrimaryAndSecondaryCtors = TestClassWithPrimaryAndSecondaryCtors("13")
    when {
        (testClassPrimary.intProp == 42).not() ->  {
            return "FAIL"
        }
        (testClassPrimary.stringProp == "42").not() ->  {
            return "FAIL"
        }
        (testClassSecondaryInt.intProp == 777).not() ->  {
            return "FAIL"
        }
        (testClassSecondaryInt.stringProp == "777").not() ->  {
            return "FAIL"
        }
        (testClassSecondaryString.intProp == 42).not() ->  {
            return "FAIL"
        }
        (testClassSecondaryString.stringProp == "13").not() ->  {
            return "FAIL"
        }
        else ->  {
            return "OK"
        }
    }
}

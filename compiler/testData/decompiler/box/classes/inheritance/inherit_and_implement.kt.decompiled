open class ParentClass(x: Int) {
    val anotherX: Int = x
}
interface MyInterface {
}
class ChildClass() : ParentClass(9), MyInterface {
}
fun box(): String  {
    val parentClass: ParentClass = ParentClass(9)
    val childClass: ChildClass = ChildClass()
    if (parentClass.anotherX == childClass.anotherX) {
        return "OK"
    }
    else {
        return "FAIL"
    }
}

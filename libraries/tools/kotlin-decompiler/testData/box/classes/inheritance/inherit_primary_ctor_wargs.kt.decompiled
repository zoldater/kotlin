// FILE: /inherit_primary_ctor_wargs.kt

open class ParentClass(final val x: Int) {
    final val anotherX: Int = this.x
}

class ChildClass() : ParentClass(9)

fun box(): String {
    val parentClass = ParentClass(9)
    val childClass = ChildClass()
    if (parentClass.anotherX == childClass.anotherX) {
        return "OK"
    } else {
        return "FAIL"
    }
}

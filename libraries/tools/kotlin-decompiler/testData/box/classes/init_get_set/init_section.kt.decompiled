class ClassWithInit() {
    val someProp : String
    val anotherProp : Int
    init {
        this.someProp = "someProp"
        this.anotherProp = 42
    }
}
fun box() : String  {
    val classWithInit : ClassWithInit = ClassWithInit()
    when {
         (classWithInit.anotherProp == 42) -> {
            return "OK"
        }
        else -> {
            return "FAIL"
        }
    }
}

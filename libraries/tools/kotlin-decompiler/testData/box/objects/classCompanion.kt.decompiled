// FILE: /classCompanion.kt

var result = ""
class A {
    companion object {
        val prop = this.test()
        fun test() : String  {
            result += "OK"
            return result
        }

    }
}
fun box() : String  {
    if  (A.prop != "OK") {
            return "fail ${A.prop}"
        }
    return result
}

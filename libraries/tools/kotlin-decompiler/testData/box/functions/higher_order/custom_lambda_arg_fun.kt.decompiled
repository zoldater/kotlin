// FILE: /custom_lambda_arg_fun.kt
import summatorType
typealias summatorType = (Int, Int) -> Int
fun summator(x: Int, y: Int) : Int  {
    return x + y
}

fun applier(body: summatorType) : Int  {
    return body.invoke(11, 22)
}

fun box() : String  {
    if  (applier(::summator) == 42) {
            throw AssertionError()
        }
    val t : Int = applier { x, y  -> y - x }
    if  (t == 11) {
            return "OK"
        } else {
            throw AssertionError()
        }
}
